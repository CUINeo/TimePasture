<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.neo.helloworld">

    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD"/>

    <application
        android:icon="@mipmap/sheephead"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/farm"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".Farm" />
        <activity android:name=".Shop" />
        <activity android:name=".FinalShop" />
        <activity android:name=".FriendList"/>
        <activity android:name=".Lyz" />
        <activity android:name=".FocusActivity" />
        <activity android:name=".PedometerActivity" />
        <service android:name=".StepService" />

        <activity
            android:name=".MenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Detail" >
        </activity>
        <activity
            android:name=".Date"
            android:label="@string/title_activity_date" >
        </activity>
        <activity
            android:name=".Plan"
            android:label="@string/title_activity_plan" >
        </activity>
        <activity
            android:name=".Progress"
            android:label="@string/title_activity_progress" >
        </activity>
        <activity
            android:name=".Todo"
            android:label="@string/title_activity_todo" >
        </activity>
        <activity
            android:name=".Tzy"
            android:label="@string/title_activity_menu" >
        </activity>

    </application>

</manifest>